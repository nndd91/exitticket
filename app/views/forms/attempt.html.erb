<h1><%= @form.title %></h1>
<ul>
<%= simple_form_for @form, url: attempting_form_path, method: :post do |f| %>
  <% array = [:q1, :q2, :q3, :q4, :q5, :q6, :q7, :q8, :q9, :q10] %>
  <% @form.questions.each_with_index do |question, index| %>
    <div class="form-inputs">
      <li><%= question.label %></li>
      <% case question.qns_type %>
      <% when 1 %>
        number 1!
        <%= f.input array[index] %>
      <% when 2 %>
        number 2!
        <%= f.input array[index], collection: (1..5) %>
      <% end %>
    </div>
  <% end %>
  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
</ul>
