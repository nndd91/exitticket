<h1>Forms index</h1>
<table class="table">
  <tr>
    <th>Title and Description</th>
    <th>Author</th>
    <th>Date</th>
    <th>Template Title</th>
    <th>Attempt</th>

  </tr>
  <% @forms.each do |form| %>
    <tr>
      <td>
        <b><%= form.title %></b><br />
        <%= form.description %>
      </td>
      <td>
        <%= form.user.first_name %>
      </td>
      <td>
        <%= form.form_date %>
      </td>
      <td>
        <%= form.form_template.title %>
      </td>
      <td>
        <!-- Click to attempt or already Done -->
        <%= link_to 'Attempt Now', attempt_form_path(form) %>
      </td>
    <tr>
  <% end %>
</table>
